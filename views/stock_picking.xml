<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data>
    <record id="view_picking_viscosity_form" model="ir.ui.view">
      <field name="name">view.picking.viscosity</field>
      <field name="model">stock.picking</field>
      <field name="inherit_id" ref="stock.view_picking_form"/>
      <field name="arch" type="xml">
        <field name="product_qty" position="after">
          <field name="conversion_rate" attrs="{'readonly': [('variable_density', '=', False)]}"/>
          <field name="variable_density"/>
          <field name="supplier_qty" readonly="True"/>
          <field name="supplier_qty_done"/>
        </field>
      </field>
    </record>
  </data>
</odoo>